{% extends "base.njk" %}

{% set file_name   = 'less_than_or_equal' %}
{% set category    = 'docs' %}
{% set subcategory = 'javascript' %}
{% set segment     = 'operators' %}
{% set subsegment  = 'less_than_or_equal' %}

{% block head %}
  <title>Оператор 'меньше или равно' в JS</title>
  <meta name="description" content="Оператор 'меньше' возвращает true, если левый операнд меньше или равен правому операнду или false если наоборот." />
  <link rel="stylesheet" href="{{prism_css}}">
  <script src="{{prism_js}}" async></script>
  <style>a[name="{{file_name}}"] {background: {{item_color}}}</style>
{% endblock %}

{% block aside_top %}{% include aside_top %}{% endblock %}
{% block aside_left %}{% include aside_left %}{% endblock %}

{% block main %}
  <h1>Меньше или равно (<=)</h1>
  <section class="content">{% include content %}</section>
{% endblock %}

{% block inlineScripts %}
  {# место для инлайновых скриптов #}
{% endblock %}