{% extends "base.njk" %}

{# variables #}
{% set title           = 'equality' %}
{% set category        = 'docs' %}
{% set subcategory     = 'javascript' %}
{% set segment         = 'operators' %}
{% set subsegment_del  = 'delete' %}

{# путь к примерам code-example-beta [НЕ УДАЛЯТЬ] #}
{% set example_dir    = 'examples/javascript/operators/' %}

{# ===================================== #}

{% block head %}
  <title>Equality operator in JavaScript</title>
  <meta name="description" content="Оператор приводит операнды к одному типу, затем применяет строгое сравнение." />
  <link rel="stylesheet" href="{{prism_css}}">
  <script src="{{prism_js}}" async></script>
{% endblock %}

{% block aside_top %}{% include aside_top %}{% endblock %}
{% block aside_left %}{% include aside_left %}{% endblock %}

{% block main %}

  <h1>{{ title | capitalize }} [ == ]</h1>
   <small>группа: операторы равенства</small>
  <p>Оператор приводит операнды к одному типу, затем применяет строгое сравнение.</p>

  {% set example_id = 'equality' %}
  {% set numeration = false %}
  {% set split      = false %}
  {% set html       = false %}
  {% set njk        = false %}
  {% set css        = false %}
  {% set js         = true %}
  {% set demo       = false %}
  {% include codeExample %}
  <table>
    <tr>
      <td>true</td>
      <td>5 равно 5</td>
    </tr>
    <tr>
      <td>true</td>
      <td>содержимое строк одинаковое</td>
    </tr>
    <tr>
      <td>false</td>
      <td>содержимое строк разное</td>
    </tr>
    <tr>
      <td>false</td>
      <td>5 не равно 4</td>
    </tr>
  </table>

{% endblock %}