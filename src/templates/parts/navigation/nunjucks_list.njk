{# variables #}
{% set path = '/docs/nunjucks/' %}

  <ul>
  {% for item in nunjucks %}
  <details>
    <summary>{{item.category}}</summary>
    <ul>
    {% for subitem in item.subcategories %}
      <li><a href="{{ path }}{{ subitem.link }}">{{ subitem.name | capitalize }}</a></li>
    {% endfor %}
    </ul>
  </details>
  {% endfor %}
  </ul>