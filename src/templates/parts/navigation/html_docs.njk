{# родительская дериктория нужна чтобы не делать длинные ссылки в data.json #}
{% set path = '/docs/html/' %}

<ul>
{% for item in html_docs %}
<details>
  <summary>{{item.category}}</summary>
  <ul>
  {% for subitem in item.subcategories %}
    <li><a href="{{ path }}{{ subitem.link }}">{{ subitem.name | capitalize }}</a></li>
  {% endfor %}
  </ul>
</details>
{% endfor %}
</ul>