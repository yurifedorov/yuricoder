{% extends "base.njk" %}

{# variables #}
{% set title        = 'приоритет операторов' %}
{% set category     = 'справочник' %}
{% set subcategory  = 'javascript' %}
{% set segment     = 'операторы' %}
{% set subsegment_del  = 'delete' %}
{% set code_language = 'language-js' %}

{# ===================================== #}

{% block head %}
  <title>Приоритет операторов</title>
  <meta name="description" content="Приоритет операторов в JavaScript." />
  <link rel="stylesheet" href="{{prism_css}}">
  <script src="{{prism_js}}" async></script>
{% endblock %}

{% block aside_top %}{% include aside_top %}{% endblock %}
{% block aside_left %}{% include aside_left %}{% endblock %}

{% block main %}

  <h1>{{ title | capitalize }}</h1>

  <section>
    <h2>Теблица</h2>
    <a href="https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Operators/Operator_Precedence">Таблица приоритетов MDN</a> <span>(где, 20 - высокий, 1 - низкий)</span>
  </section>

{% endblock %}