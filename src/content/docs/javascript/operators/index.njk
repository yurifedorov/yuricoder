{% extends "base.njk" %}

{# variables #}
{% set title           = 'operators' %}
{% set category        = 'docs' %}
{% set subcategory     = 'javascript' %}
{% set segment_del     = 'delete' %}
{% set subsegment_del  = 'delete' %}

{# ===================================== #}

{% block head %}
  <title>All operators in JavaScript</title>
  <meta name="description" content="List of all javascript operators with usage examples." />
{% endblock %}

{% block aside_top %}{% include aside_top %}{% endblock %}
{% block aside_left %}{% include aside_left %}{% endblock %}

{% block main %}

  <h1>All operators in JavaScript</h1>

  <section>
    <h2>Арифметические операторы</h2>
    <ul class="tile">
      {% for item in arithmetic_operators %}
      <li>
        <div>
          <a href="{{item.link}}">{{item.name}}</a>
          <p>{{item.description}}</p>
        </div>
      </li>
      {% endfor %}
    </ul>
  </section>

  <section>
    <h2>Инкремент и декремент</h2>
    <ul class="tile">
      {% for item in increment_and_decrement %}
      <li>
        <div>
          <a href="{{item.link}}">{{item.name}}</a>
          <p>{{item.description}}</p>
        </div>
      </li>
      {% endfor %}
    </ul>
  </section>


  <h3>Исходники</h3>

