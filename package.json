  "devDependencies": {
    "browser-sync": "^2.26.3",
    "commitizen": "^3.0.5",
    "commitlint": "^7.2.1",
    "cz-customizable": "^5.3.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-data": "^1.3.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-minify": "^3.1.0",
    "gulp-nunjucks-render": "^2.2.2",
    "gulp-rename": "^1.4.0",
    "husky": "^1.2.0"
  }
}
