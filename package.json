{
  "name": "yuricoder",
  "version": "1.0.0",
  "description": "My personal webpage",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuryfyodorov/yuricoder.git"
  },
  "private": true,
  "author": {
    "name": "Yuri Fedorov",
